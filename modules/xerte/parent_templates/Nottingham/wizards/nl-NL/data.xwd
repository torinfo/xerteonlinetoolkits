<?xml version="1.0"?>
<wizard menus="Tekst,Media,Navigatoren,Connectoren,Grafieken,Interactiviteit,Spelletjes,Links / Feeds,Tracking / xAPI">

  <learningObject icon="icPackage" remove="false" duplicate="false">

    <name type="TextInput" wysiwyg="true" label="Titel leerobject"/>
    <navigation type="ComboBox" options="Lineair,Historisch,Menu,Menu met pagina knoppen,Lineair met historische terugknop" data="Linear,Historic,Menu,Menu with Page Controls,LinearWithHistoric" label="Navigatie" width="170" conditionTrigger="true"/>
    <language type="LanguageList" label="Taal" width="170"/>
    <theme type="ThemeList" label="Thema" mandatory="true" conditionTrigger="true" defaultValue="default"/>
	<themeIcons type="CheckBox" label="Update thema iconen" mandatory="true" defaultValue="false" condition="theme_list[currtheme].imgbtns=='true'||theme==undefined||theme=='default'" tooltip="Het geselecteerde thema gebruikt afbeeldingen voor de knoppen voor de navigatie en voetbalkknoppen. De meeste nieuwere thema's gebruiken in plaats daarvan Font Awesome-iconen, die je meer flexibiliteit geven in hun uiterlijk. Als je&amp;nbsp;dit vakje aanvinkt, worden de knoppen automatisch bijgewerkt naar&amp;nbsp;Font Awesome-iconen. Het wordt aanbevolen om dit aan te vinken als je&amp;nbsp;een van de opties Knop-iconen&amp;nbsp;aanpassen gebruikt, zodat alle knoppen consistent lijken qua uiterlijk."/>
	<displayMode type="ComboBox" options="Standaard, Volledig venster" data="default,fill window" label="Weergave modus" width="100" conditionTrigger="true"/>
	<responsive type="CheckBox" label="Responsive tekst" defaultValue="false" mandatory="true" conditionTrigger="true"/>
	<textSize type="ComboBox" options="10,12,14,16,18,24,36" label="Tekst grootte" width="100" tooltip="De standaard tekstgrootte wordt alleen gebruikt als responsieve tekst is uitgeschakeld. Het kan ook worden overschreven wanneer een tekstgrootte rechtstreeks in de teksteditor is ingesteld" condition="responsive!='true'"/>
	
	<!-- OPTIONAL PROPERTIES -->
	
	<maxWidth optional="true" type="NumericStepper" min="800" max="2000" step="10" label="Maximale breedte" defaultValue="1400" tooltip="Gebruik deze optie om de maximale breedte (bijvoorbeeld 1400px) in te stellen waarmee de pagina-inhoud op grotere schermen wordt weergegeven. Opmerking: sommige pagina's, zoals medialessen en hotspot-connector, vullen momenteel nog de volledige breedte van de pagina."/>
	<fixDisplay optional="true" type="TextInput" label="Vaste scherm afmetingen" defaultValue="800,600" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')"/>
	
	<fullScreenGroup type="group" label="Knop volledig scherm" optional="true" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" tooltip="Pas de tooltipbeschrijvingen en iconen aan die worden gebruikt op de knop voor&amp;nbsp;volledig scherm">
		<fullScreenIcons type="CheckBox" label="Knop aanpassen icoon" defaultValue="false" conditionTrigger="true" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" optional="true" tooltip="Negeer het standaardicoon&amp;nbsp;op de volledig scherm knop. Wanneer je deze&amp;nbsp;aangevinkt, zie je&amp;nbsp;meer opties waarmee je het&amp;nbsp;icoon&amp;nbsp;kunt selecteren. Als dit niet is aangevinkt, is&amp;nbsp;de afbeelding/icoon&amp;nbsp;die van het&amp;nbsp;thema zelf."/>
		<maxScreenGroup type="group" label="Volledig scherm" optional="true">
			<maxLabel label="Tooltip" type="TextInput" defaultValue="Volledig scherm" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" optional="true"/>
			<maxIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-expand-arrows-alt" condition="(displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!=''))&amp;&amp;fullScreenIcons=='true'" optional="true"/>
		</maxScreenGroup>
		<minScreenGroup type="group" label="Vaste grootte" optional="true">
			<minLabel label="Tooltip" type="TextInput" defaultValue="Standaard" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" optional="true"/>
			<minIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-compress-arrows-alt" condition="(displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!=''))&amp;&amp;fullScreenIcons=='true'" optional="true"/>
		</minScreenGroup>
	</fullScreenGroup>
	
	<hideHeader optional="true" type="CheckBox" label="Verberg titelbalk" defaultValue="false"/>
	<hideFooter optional="true" type="CheckBox" label="Verberg voetbalk" defaultValue="false"/>
	
	<navGroup type="group" label="Navigatie knoppen" optional="true" tooltip="Pas de tooltipbeschrijvingen en iconen aan die worden gebruikt voor&amp;nbsp;de&amp;nbsp;navigatie knoppen">
		<navIcons type="CheckBox" label="Knop aanpassen icoon" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Negeer de standaardiconen die op de navigatieknoppen worden gebruikt. Wanneer je dit aangevinkt, zie je&amp;nbsp;meer opties waarmee je&amp;nbsp;de gebruikte iconen&amp;nbsp;kunt selecteren. Als dit niet is aangevinkt, worden de afbeeldingen/iconen&amp;nbsp;gebruikt van het&amp;nbsp;thema zelf."/>
		<prevGroup type="group" label="Vorige pagina" optional="true">
			<prevLabel label="Tooltip" type="TextInput" defaultValue="Terug" condition="navigation!='Menu'" optional="true"/>
			<prevIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-chevron-circle-left" condition="navIcons=='true'&amp;&amp;navigation!='Menu'" optional="true"/>
		</prevGroup>
		<nextGroup type="group" label="Volgende pagina" optional="true">
			<nextLabel label="Tooltip" type="TextInput" defaultValue="Volgende" condition="navigation!='Menu'" optional="true"/>
			<nextIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-chevron-circle-right" condition="navIcons=='true'&amp;&amp;navigation!='Menu'" optional="true"/>
		</nextGroup>
		<tocGroup type="group" label="Inhoudsopgave" optional="true">
			<tocLabel label="Tooltip" type="TextInput" defaultValue="Inhoud" condition="navigation!='Historic'" optional="true" tooltip="De tooltip voor de knop Inhoudsopgave. Deze tekst verschijnt ook als titel van het dialoogvenster dat wordt geopend wanneer op de knop wordt geklikt (tenzij de navigatie is ingesteld op menu of menu met paginaknoppen)."/>
			<tocIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-bars" condition="navIcons=='true'&amp;&amp;navigation!='Historic'" optional="true"/>
		</tocGroup>
		<homeGroup type="group" label="Home pagina" optional="true">
			<homeLabel label="Tooltip" type="TextInput" defaultValue="Home" condition="navigation=='Historic'" optional="true"/>
			<homeIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-home" condition="navIcons=='true'&amp;&amp;navigation=='Historic'" optional="true"/>
		</homeGroup>
	</navGroup>
	
	<toc type="group" label="Inhoudsopgave" optional="true">
		<tocNumbers optional="true" type="CheckBox" label="Pagina nummering" defaultValue="true" condition="navigation!='Historic'"/>
		<pageTick optional="true" type="CheckBox" label="Markeer pagina bekeken" defaultValue="true" condition="navigation!='Historic'"/>
		<tocTarget label="Toon in" type="ComboBox" data="dialog,lightbox" options="Dialoog venster,Lightbox" defaultValue="dialog" condition="navigation=='Linear'||navigation=='LinearWithHistoric'||navigation==undefined" optional="true" tooltip="bepaalt waar de inhoudsopgave wordt geopend"/>
		<menuImage optional="true" type="media" label="Pagina afbeelding" defaultValue="Selecteer een afbeelding" condition="navigation=='Menu'||navigation=='Menu with Page Controls'"/>
		<menuImageProperties optional="true" type="ComboBox" label="Stijl pagina afbeelding" options="Gebruik paneel,Volledig scherm" data="Use Panel,Full Screen" width="100" defaultValue="Use Panel" condition="navigation=='Menu'||navigation=='Menu with Page Controls'"/>
	</toc>
	
	<homePageGroup type="group" label="Voorpagina" optional="true">
		<homePageLink optional="true" type="CheckBox" label="Link de titel van het leerobject naar de voorpagina" defaultValue="false" conditionTrigger="true" tooltip="Maak van de titel van het leerobject een link die gebruikers terugstuurt naar de voorpagina"/>
		<homePage optional="true" type="pagelist" label="Voorpagina" defaultValue="" condition="(navigation=='Historic'||homePageLink!='false')&amp;&amp;navigation!='Menu'&amp;&amp;navigation!='Menu with Page Controls'" tooltip="Standaard is de voorpagina de eerste pagina in het leerobject. Selecteer een pagina in de lijst om in plaats daarvan een andere pagina in het leerobject als voorpaginastartpagina te gebruiken."/>
	</homePageGroup>
	
	<progressBar optional="true" type="ComboBox" options="Toon voortgangsbalk met paginanummers,Toon voortgangsbalk en verberg paginanummers" data="pBar,pBarNoCounter" label="Voortgangsbalk" defaultValue="pBar" width="100"/>
	
	<footerToolsGroup type="group" label="Toon / Verberg footer knoppen" optional="true" tooltip="De voetbalk-tools zijn de knoppen die normaal linksonder in het leerobject verschijnen, zoals toegankelijkheids-opties en&amp;nbsp; de woordenlijst. Met de knop Verberg/toon voetbalk knoppen kun je&amp;nbsp;deze knoppen in en uit het zicht zetten.">
		<footerTools type="ComboBox" label="Standaard status" options="Verberg footer knoppen,Toon footer knoppen,Verwijder toon/verberg knop" data="hideFooterTools,showFooterTools,none" conditionTrigger="true" defaultValue="showFooterTools" width="100" optional="true"/>
		<footerToolIcons type="CheckBox" label="Knop aanpassen icoon" defaultValue="false" conditionTrigger="true" condition="footerTools!='none'" optional="true" tooltip="Negeer de standaardiconen die op de toon / verberg footer knoppen worden gebruikt. Wanneer je dit&amp;nbsp;aangevinkt, zie je&amp;nbsp;meer opties waarmee je&amp;nbsp;de gebruikte iconen&amp;nbsp;kunt selecteren. Als dit niet is aangevinkt, worden de afbeeldingen/iconen&amp;nbsp;gebruikt van het&amp;nbsp;thema zelf."/>
		<hideToolsGroup type="group" label="Verberg footer knoppen" optional="true">
			<hideToolsLabel label="Tooltip" type="TextInput" defaultValue="Verberg footer knoppen" condition="footerTools!='none'" optional="true"/>
			<hideToolsIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-left" condition="footerTools!='none'&amp;&amp;footerToolIcons=='true'" optional="true"/>
		</hideToolsGroup>
		<showToolsGroup type="group" label="Toon footer knoppen" optional="true">
			<showToolsLabel label="Tooltip" type="TextInput" defaultValue="Toon footer knoppen" condition="footerTools!='none'" optional="true"/>
			<showToolsIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-right" condition="footerTools!='none'&amp;&amp;footerToolIcons=='true'" optional="true"/>
		</showToolsGroup>
	</footerToolsGroup>
	
	<accessibilityGroup type="group" label="Toegankelijkheid" optional="true" tooltip="Opties met betrekking tot de knop Toegankelijkheid">
		<accessibilityLabel label="Tooltip" type="TextInput" defaultValue="Toegankelijkheids opties" condition="accessibilityHide!='true'" optional="true" tooltip="De tooltip voor de knop Toegankelijkheid. Deze tekst verschijnt ook als de titel van het dialoogvenster dat wordt geopend wanneer op de knop wordt geklikt."/>
		<accessibilityTarget label="Toon in" type="ComboBox" data="dialog,lightbox" options="Dialoog venster,Lightbox" defaultValue="dialog" condition="accessibilityHide!='true'" optional="true" tooltip="bepaalt waar de toegankelijkheids opties worden geopend"/>
		<accessibilityIc type="CheckBox" label="Knop aanpassen icoon" defaultValue="false" conditionTrigger="true" condition="accessibilityHide!='true'" optional="true" tooltip="Negeer de standaardiconen die op de toegankelijkheidsknoppen worden gebruikt. Wanneer je dit&amp;nbsp;aangevinkt, zie je&amp;nbsp;meer opties waarmee je&amp;nbsp;de gebruikte iconen&amp;nbsp;kunt selecteren. Als dit niet is aangevinkt, worden de afbeeldingen/iconen&amp;nbsp;gebruikt van het&amp;nbsp;thema zelf."/>
		<accessibilityIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-eye-slash" condition="accessibilityHide!='true'&amp;&amp;accessibilityIc=='true'" optional="true"/>
		<wcagGroup type="group" label="Logo en link" optional="true" tooltip="Verberg het toegankelijkheids logo en link of pas de link aan.">
			<wcagHide label="Verberg logo en link" type="Checkbox" conditionTrigger="true" defaultValue="false" condition="accessibilityHide!='true'" optional="true"/>
			<wcagAlt label="Logo alt text" type="TextInput" defaultValue="WCAG WAI-AA logo" condition="accessibilityHide!='true'&amp;&amp;wcagHide=='false'" optional="true"/>
			<wcagLinkTitle label="Link Tooltip" type="TextInput" defaultValue="Bekijk de Xerte toegankelijkheidsverklaring" condition="accessibilityHide!='true'&amp;&amp;wcagHide=='false'" optional="true"/>
			<wcagLink type="Media" label="Toegankelijkheidsverklaring link" tooltip="Voeg een url toe of blader naar een bestand om te linken naar je eigen toegankelijkheidsverklaring. Laat leeg om te linken naar de toegankelijkheidsverklaring op xerte.org.uk." condition="accessibilityHide!='true'&amp;&amp;wcagHide=='false'" optional="true"/>
		</wcagGroup>
		<accessibilityHide type="CheckBox" label="Verberg knop" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Gebruik deze optie met de nodige voorzichtigheid! Hoewel je de knop Toegankelijkheidsopties in het leerobject kunt verbergen, moet je goed nadenken voordat je&amp;nbsp;dit doet. Met de toegankelijkheidsopties kunnen deelnemers&amp;nbsp;het uiterlijk van het leerobject eenvoudig aanpassen aan hun behoeften en links vinden naar informatie over de toegankelijkheid van Xerte."/>
	</accessibilityGroup>
	
	<helpGroup type="group" label="Help bestand" optional="true" tooltip="Voeg een help-knop toe aan de voetbalk, waarmee een bestand of URL wordt geopend wanneer erop geklikt wordt">
		<nfo label="Bestand" type="media" optional="true" tooltip="Upload bestand (bv. pdf) of voeg een url toe"/>
		<helpTarget type="ComboBox" label="Open in" defaultValue="new" options="Nieuw venster,Lightbox" data="new,lightbox" tooltip="Waar het helpbestand wordt geopend - standaard wordt dit geopend in een nieuwe tab / -venster&amp;nbsp;" optional="true"/>
		<helpLabel label="Titel" type="TextInput" defaultValue="Help" optional="true"/>
		<helpIc type="CheckBox" label="Knop aanpassen icoon" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Negeer de standaardiconen die op de helpbestand knop&amp;nbsp;wordt&amp;nbsp;gebruikt. Wanneer je dit&amp;nbsp;aangevinkt, zie je&amp;nbsp;meer opties waarmee je het icoon&amp;nbsp;kunt selecteren. Als dit niet is aangevinkt, wordt de afbeeldingen/iconen&amp;nbsp;gebruiket van het&amp;nbsp;thema zelf."/>
		<helpIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-question" condition="helpIc=='true'" optional="true"/>
	</helpGroup>
	
	<introGroup type="group" label="Leerobject introductie" optional="true" tooltip="Voeg een knop Leerobject Introductie toe aan de voetbalk, die informatie opent in een lightbox wanneer erop wordt geklikt. Deze introductie kan zo worden ingesteld dat deze automatisch wordt geopend wanneer het leerobject voor het eerst wordt geopend.">
		<introType type="ComboBox" label="Content type" defaultValue="text" options="Tekst,Afbeelding,Video,URL,Bestand" data="text,image,video,url,file" conditionTrigger="true" optional="true" tooltip="Het type inhoud dat wordt gebruikt als introductie voor dit leerobject."/>
		<intro label="Tekst" type="TextArea" height="100" defaultValue="" condition="introType=='text'||introType==undefined" optional="true"/>
		<introImg label="Afbeelding" type="media" height="100" defaultValue="" condition="introType=='image'" optional="true"/>
		<introVideo label="Video" type="media" height="100" defaultValue="" condition="introType=='video'" optional="true"/>
		<introTip label="Beschrijving" type="TextInput" defaultValue="Voeg toegankelijkheids info toe" placeholder="" condition="introType=='image'||introType=='video'" optional="true"/>
		<introURL label="URL" type="TextInput" defaultValue="" condition="introType=='url'" optional="true"/>
		<introFile label="Bestand" type="media" height="100" defaultValue="" condition="introType=='file'" optional="true"/>
		<introTitle type="CheckBox" label="Titel weergeven" defaultValue="true" condition="introType=='text'||introType==undefined||introType=='image'" optional="true" tooltip="Toon de titel van het leerobject boven de introductietekst"/>
		<introBtn type="CheckBox" label="Start knop" defaultValue="true" conditionTrigger="true" condition="introType=='text'||introType==undefined||introType=='image'" optional="true" tooltip="Voeg onder de introductietekst een 'start'-knop toe die de lightbox sluit wanneer erop wordt geklikt"/>
		<introBtnTxt label="Knop tekst" type="TextInput" defaultValue="Aan de slag" condition="introBtn=='true'&amp;&amp;(introType=='text'||introType==undefined||introType=='image')" wysiwyg="true" optional="true"/>
		<introWidth label="Breedte lightbox" options="Auto,Klein,Medium,Groot,Volledig" type="ComboBox" data="Auto,Small,Medium,Large,Full" defaultValue="Auto" condition="introType=='text'||introType==undefined" optional="true"/>
		<introShow type="ComboBox" label="Standaard status" defaultValue="first" options="Open,Dicht" data="always,first,never" optional="true" tooltip="Geef aan of de introductie automatisch wordt geopend wanneer het leerobject voor het eerst wordt geopend"/>
		<introBtnGroup type="group" label="Knop" optional="true">
			<introLabel label="Tooltip" type="TextInput" defaultValue="Introductie" tooltip="De tooltip voor de knop Leerobject Introductie" optional="true"/>
			<introIc type="CheckBox" label="Knop aanpassen icoon" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Negeer het standaardicoon op de leerobject introductie&amp;nbsp;knop. Wanneer je deze&amp;nbsp;aangevinkt, zie je&amp;nbsp;meer opties waarmee je het&amp;nbsp;icoon&amp;nbsp;kunt selecteren. Als dit niet is aangevinkt, is&amp;nbsp;de afbeelding/icoon&amp;nbsp;die van het&amp;nbsp;thema zelf."/>
			<introIcon label="Icoon" type="FontAwesome" iconList="hotspot" condition="introIc!='false'" defaultValue="fas fa-info" optional="true"/>
		</introBtnGroup>
	</introGroup>
	
	<pageIntroGroup type="group" label="Pagina introductie knop" optional="true" tooltip="Er verschijnt een extra knop in de interface als je helpbestanden aan een van de&amp;nbsp;pagina's hebt toegevoegd. Gebruik deze eigenschappen om de plaats&amp;nbsp;en het uiterlijk van deze knop te bepalen.">
		<pageIntroBg label="Uiterlijk" type="ComboBox" options="Alleen icoon, Cirkel met icoon" data="icon,circle" defaultValue="circle" optional="true"/>
		<pageIntroLabel label="Tooltip" type="TextInput" defaultValue="Pagina introductie" optional="true"/>
		<pageIntroIc type="CheckBox" label="Knop aanpassen icoon" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Negeer het standaardicoon&amp;nbsp;op de pagina introductie&amp;nbsp;knop. Wanneer je deze&amp;nbsp;aangevinkt, zie je&amp;nbsp;meer opties waarmee je het&amp;nbsp;icoon&amp;nbsp;kunt selecteren. Als dit niet is aangevinkt, is&amp;nbsp;de afbeelding/icoon&amp;nbsp;die van het&amp;nbsp;thema zelf."/>
		<pageIntroIcon label="Icoon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-info" condition="pageIntroIc=='true'" optional="true"/>
	</pageIntroGroup>
	
	<sideBarGroup type="group" label="Zijbalk" optional="true" tooltip="Voeg een inklapbare zijbalk toe aan het leerobject met knoppen die normaal gesproken op de voetbalk te vinden zijn.">
		<sideBar type="CheckBox" label="Maak een zijbalk" defaultValue="true" conditionTrigger="true" optional="true"/>
		<sideBarPosition type="ComboBox" label="Positie" defaultValue="left" options="Links,Rechts" data="left,right" conditionTrigger="true" condition="sideBar!='false'" optional="true"/>
		<sideBarShow type="ComboBox" label="Standaard status" defaultValue="open" options="Open,Gesloten" data="open,closed" condition="sideBar!='false'" optional="true"/>
		<sideBarBtns type="group" label="Zijbalk knoppen" condition="sideBar!='false'" cols="2" optional="true">
			<sbGlossary label="Woordenlijst" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbAccessibility label="Toegankelijkheid" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbToC label="Inhoudsopgave" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbProjectHelp label="Leerobject help" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbProjectIntro label="Leerobject Introductie" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbPageIntro label="Pagina introductie" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
		</sideBarBtns>
		<sideBarBtnTxt type="CheckBox" label="Toon titel knop" defaultValue="true" condition="sideBar!='false'" optional="true"/>
		<sideBarSize type="ComboBox" label="Grootte knop" defaultValue="small" options="Klein,Groot" data="small,large" condition="sideBar!='false'" optional="true"/>
		<sideBarHideShowGroup type="group" label="Toon / Verberg zijbalk knoppen" optional="true">
			<sideBarBtnIcons type="CheckBox" label="Knop aanpassen icoon" defaultValue="false" conditionTrigger="true" condition="sideBar!='false'" optional="true" tooltip="Negeer het standaardicoon&amp;nbsp;op de toon / verberg zijbalk knop. Wanneer je deze&amp;nbsp;aangevinkt, zie je&amp;nbsp;meer opties waarmee je het&amp;nbsp;icoon&amp;nbsp;kunt selecteren. Als dit niet is aangevinkt, is&amp;nbsp;de afbeelding/icoon&amp;nbsp;die van het&amp;nbsp;thema zelf."/>
			<sideBarHideGroup type="group" label="Verberg zijbalk" optional="true">
				<sideBarHideLabel label="Tooltip" type="TextInput" defaultValue="Hide side bar" condition="sideBar!='false'" optional="true"/>
				<sideBarHideLeftIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-left" condition="sideBar!='false'&amp;&amp;sideBarBtnIcons=='true'&amp;&amp;sideBarPosition=='left'" optional="true"/>
				<sideBarHideRightIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-right" condition="sideBar!='false'&amp;&amp;sideBarBtnIcons=='true'&amp;&amp;sideBarPosition=='right'" optional="true"/>
			</sideBarHideGroup>
			<sideBarShowGroup type="group" label="Toon zijbalk" optional="true">
				<sideBarShowLabel label="Tooltip" type="TextInput" defaultValue="Show side bar" condition="sideBar!='false'" optional="true"/>
				<sideBarShowLeftIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-right" condition="sideBar!='false'&amp;&amp;sideBarBtnIcons=='true'&amp;&amp;sideBarPosition=='left'" optional="true"/>
				<sideBarShowRightIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-left" condition="sideBar!='false'&amp;&amp;sideBarBtnIcons=='true'&amp;&amp;sideBarPosition=='right'" optional="true"/>
			</sideBarShowGroup>
		</sideBarHideShowGroup>
	</sideBarGroup>
	
	<icon type="group" label="Header icoon/ logo" optional="true">
		<icHide label="Verberg logo" type="Checkbox" defaultValue="false" optional="true" conditionTrigger="true" tooltip="Als er automatisch een standaardlogo wordt toegevoegd door  het template, of door het thema dat je hebt gekozen, gebruik dan dit selectievakje om dat logo te verbergen, of laat dit niet aangevinkt en gebruik de onderstaande opties om een alternatief logo te laden."/>
		<ic optional="true" type="media" label="Afbeelding" condition="icHide=='false'" defaultValue="Selecteer een afbeelding"/>
		<icTip optional="true" type="TextInput" label="Beschrijving" condition="icHide=='false'" defaultValue="Voeg toegankelijkheids info toe" placeholder=""/>
		<icPosition optional="true" type="ComboBox" options="Links,Rechts" data="left,right" condition="icHide=='false'" label="Positie" defaultValue="left" width="100"/>
	</icon>
	
	<bg type="group" label="Achtergrond" optional="true">
