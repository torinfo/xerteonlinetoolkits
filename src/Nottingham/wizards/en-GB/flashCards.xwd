<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>      <flashCards><![CDATA[<flashCards name="Enter Page Title" text="Enter text for the page here" cardNoTxt="Card {i} of {n}" flipTxt="Flip Card" nextTxt="Next" prevTxt="Previous" backScrollTip="Scroll Back" nextScrollTip="Scroll Forward"	  />]]></flashCards>          </newNodes>  </pageWizard>  <!-- FLASH CARDS=============================== -->    <flashCards menu="Navigators" menuItem="Flash Cards" icon="icRefreshPage" remove="true" thumb="thumbs/flashCards.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#flashcards" wiki="https://xerte.org.uk/wiki/doku.php?id=navigator_flashcards_page" hint="A series of double sided flash cards containing text and media.">	    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea" height="120"/>	<random label="Randomise Cards" type="Checkbox" optional="true" tooltip="Randomise the order of the cards" />    <amountToShow label="Cards To Show" type="NumericStepper" min="1" max="1000" step="1" defaultValue="1" optional="true" tooltip="Use with 'Randomise Cards' to show a limited number of randomly selected cards"/>	<displayStyle label="Card Layout" type="ComboBox" options="One at a time,In a row,In a column" data="one,horizontal,vertical" optional="true" defaultValue="one" tooltip="Display cards one at a time or in a row/column"/>	<sizing type="group" label="Card Size" optional="true">		<cardSize label="Width" type="ComboBox" options="Small,Medium,Large,Full" data="small,medium,large,full" defaultValue="medium" width="100" optional="true"/>		<cardHeight label="Height" type="ComboBox" options="Fill Space,Fit to Content,Match Tallest Card" data="default,fit,match" defaultValue="default" width="100" optional="true"/>	</sizing>		<cardNoTxt label="Card No. Text" type="TextInput" language="true" />    <flipTxt label="Flip Button Label" type="TextInput" language="true"/>	<nextTxt label="Next Button Label" type="TextInput" language="true" />	<prevTxt label="Previous Button Label" type="TextInput" language="true" />	<backScrollTip label="Scroll Back Label" type="TextInput" wysiwyg="true" language="true" />    <nextScrollTip label="Scroll Forward Label" type="TextInput" wysiwyg="true" language="true" />	<AIGroup type="group" label="AI" tooltip ="Generate content automatically with the help of AI." optional="true">    	  <aiSelector label="AI model" options="OpenAI,Claude" type="comboBox" data="openai,copilot" defaultValue="openai" width="120" optional="true"/>          <subject label="Subject" type="TextInput" wysiwyg="true" tooltip = "This determines which area or subject the generated words and definitions will follow. More specific subjects may result in less consistent or reliable content. Examples: 'Birds', 'Terrestrial birds', 'Birds native to the Netherlands', 'Most common birds in the urban areas of the Netherlands'." optional="true"/>          <amountOfCards label="Amount Of Flashcards" type="TextInput" tooltip = "The amount of flash cards the AI will generate." wysiwyg="true" optional="true"/>          <ageRange label="Reading Level" type="TextInput" tooltip="The estimated age, grade, education or other qualifier for which the AI will attempt to curate the content. Examples: students aged 12-16, grade 7 students in the Netherlands, university graduates, office workers in the education field, A2 level Dutch speakers, etc." wysiwyg="true" optional="true"/>          <hintMode label="Include hints" options="YES,NO" type="comboBox" data="ON,OFF" defaultValue="OFF" tooltip = "Add a hint on the first side of the flashcard to help with guessing the word." width="120" optional="true"/>          <languageMode label="Language Learning Mode" options="ON,OFF" type="comboBox" data="ON,OFF" defaultValue="OFF" tooltip = "Customize the cards to aid with language learning. Ensures the word being guessed is in the target language." width="120" optional="true"/>          <languageChoice label="Language Choice" type="TextInput" tooltip = "List the target language, with the native (speaking) language being English. Example: 'Spanish' will create hints/descriptions in English while the words to be guessed will be in Spanish." wysiwyg="true" optional="true"/>          <reverseMode label="Content Order" options="Definitions First, Word First" type="comboBox" data="ON,OFF" defaultValue="OFF" tooltip = "When 'Definitions First' is selected, the definition will show up first, and the player will have to guess which word is on the flip-side of the card. When 'Word First' is selected, the player will see the word and will have to describe what the word means or give an appropriate definition." width="120" optional="true"/>          <voiceSelector label="Tone and Style" type="TextInput" wysiwyg="true" tooltip = "Adjusts the tone and style of the generated content. Possibilities include: formal, semi-formal, informal, semi-informal, active, passive as well as combinations. Additional modifiers are possible, but may result in less consistent content. Examples: 'Semi-formal teaching, active voice', 'formal, passive voice, usage instructions'." optional="true"/>          <uploadPrompt label="Upload file?" type="checkbox" defaultValue="false" tooltip="Check to upload a file. Please ensure the uploaded file corresponds to the requested subject. If relevant information is not found in the file, the AI will attempt to supplement with its own knowledge." optional ="true"/>          <file label="File" type="media" defaultValue="Upload a file" tooltip = "Select a video, audio, powerpoint, PDF or other text file to be used for content generation." optional="true"/>          <fileAccessPrompt label="Allow information supplementation" type="checkbox" defaultValue="false" tooltip = "Instruct the AI to use information beyond what was provided in the file, using its own knowledge stores. Keep in mind that this may result in inaccurate information and should be thoroughly double-checked!" optional ="true"/>          <aiButton type="aibutton" optional="true"/>          <explanation label="This content is generated using AI. Make sure to double-check and adjust!" type="info" optional="true"/>    </AIGroup>    <newNodes>      <card><![CDATA[<card name="Enter Title Here" side1="Content for side one of the card" side2="Content for side two of the card"/>]]></card>    </newNodes>	  </flashCards>    <card menuItem="Card" icon="icTwoPages" remove="true">	<name label="Title" type="TextInput" wysiwyg="true"/>	<side1 label="Side 1 Text" type="TextArea" height="120"/>	<side2 label="Side 2 Text" type="TextArea" height="120"/>		<side1Media type="group" label="Side 1 Media" optional="true" tooltip="Media (e.g. image) shown on the front of this card">		<media1 label="File" type="Media" optional="true" defaultValue="Select a File"/>		<tip1 label="Description" type="textInput" optional="true" defaultValue="" placeholder="Enter a description for accessibility"/>		<align1 label="Position" options="Top,Bottom" type="ComboBox" optional="true" data="Top,Bottom" defaultValue="Bottom" width="100"/>		<transcript1 label="Audio Transcript" type="textArea" height="100" defaultValue="" optional="true" tooltip="If media if MP3, a transcript button will appear on the audio player when text is added here"/>	</side1Media>		<side2Media type="group" label="Side 2 Media" optional="true" tooltip="Media (e.g. image) shown on the back of this card">		<media2 label="File" type="Media" optional="true" defaultValue="Select a File"/>		<tip2 label="Description" type="textInput" optional="true" defaultValue="" placeholder="Enter a description for accessibility"/>		<align2 label="Position" options="Top,Bottom" type="ComboBox" optional="true" data="Top,Bottom" defaultValue="Bottom" width="100"/>		<transcript2 label="Audio Transcript" type="textArea" height="100" defaultValue="" optional="true" tooltip="If media if MP3, a transcript button will appear on the audio player when text is added here"/>	</side2Media>	  </card></wizard>