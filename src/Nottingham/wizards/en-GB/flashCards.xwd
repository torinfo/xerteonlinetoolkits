<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>      <flashCards><![CDATA[<flashCards name="Enter Page Title" text="Enter text for the page here" cardNoTxt="Card {i} of {n}" flipTxt="Flip Card" nextTxt="Next" prevTxt="Previous" backScrollTip="Scroll Back" nextScrollTip="Scroll Forward"	  />]]></flashCards>          </newNodes>  </pageWizard>  <!-- FLASH CARDS=============================== -->    <flashCards menu="Navigators" menuItem="Flash Cards" icon="icRefreshPage" remove="true" thumb="thumbs/flashCards.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#flashcards" wiki="https://xerte.org.uk/wiki/doku.php?id=navigator_flashcards_page" hint="A series of double sided flash cards containing text and media.">	    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea" height="120"/>	<random label="Randomise Cards" type="Checkbox" optional="true" tooltip="Randomise the order of the cards" />    <amountToShow label="Cards To Show" type="NumericStepper" min="1" max="1000" step="1" defaultValue="1" optional="true" tooltip="Use with 'Randomise Cards' to show a limited number of randomly selected cards"/>	<displayStyle label="Card Layout" type="ComboBox" options="One at a time,In a row,In a column" data="one,horizontal,vertical" optional="true" defaultValue="one" tooltip="Display cards one at a time or in a row/column"/>	<sizing type="group" label="Card Size" optional="true">		<cardSize label="Width" type="ComboBox" options="Small,Medium,Large,Full" data="small,medium,large,full" defaultValue="medium" width="100" optional="true"/>		<cardHeight label="Height" type="ComboBox" options="Fill Space,Fit to Content,Match Tallest Card" data="default,fit,match" defaultValue="default" width="100" optional="true"/>	</sizing>		<cardNoTxt label="Card No. Text" type="TextInput" language="true" />    <flipTxt label="Flip Button Label" type="TextInput" language="true"/>	<nextTxt label="Next Button Label" type="TextInput" language="true" />	<prevTxt label="Previous Button Label" type="TextInput" language="true" />	<backScrollTip label="Scroll Back Label" type="TextInput" wysiwyg="true" language="true" />    <nextScrollTip label="Scroll Forward Label" type="TextInput" wysiwyg="true" language="true" />	<AIGroup type="group" label="AI" optional="true">    	  <aiSelector label="AI model" options="OpenAI,Copilot" type="comboBox" data="openai,copilot" defaultValue="openai" width="120" optional="true"/>          <subject label="Subject" type="TextInput" wysiwyg="true" optional="true"/>          <amountOfCards label="Amount Of Flashcards" type="TextInput" wysiwyg="true" optional="true"/>          <ageRange label="Age Range" type="TextInput" wysiwyg="true" optional="true"/>          <hintMode label="Add hints to help guess the word?" options="YES,NO" type="comboBox" data="ON,OFF" defaultValue="OFF" width="120" optional="true"/>          <languageMode label="Create cards for language learning" options="YES,NO" type="comboBox" data="ON,OFF" defaultValue="OFF" width="120" optional="true"/>          <languageChoice label="Choose which language to learn" type="TextInput" wysiwyg="true" optional="true"/>          <reverseMode label="Definitions first?" options="YES,NO" type="comboBox" data="ON,OFF" defaultValue="OFF" width="120" optional="true"/>          <img label="File" type="media" defaultValue="Upload a file" optional="true"/>          <uploadPrompt label="Upload file?" type="checkbox" defaultValue="false" optional ="true"/>          <aiButton type="aibutton" optional="true"/>    </AIGroup>    <newNodes>      <card><![CDATA[<card name="Enter Title Here" side1="Content for side one of the card" side2="Content for side two of the card"/>]]></card>    </newNodes>	  </flashCards>    <card menuItem="Card" icon="icTwoPages" remove="true">	<name label="Title" type="TextInput" wysiwyg="true"/>	<side1 label="Side 1 Text" type="TextArea" height="120"/>	<side2 label="Side 2 Text" type="TextArea" height="120"/>		<side1Media type="group" label="Side 1 Media" optional="true" tooltip="Media (e.g. image) shown on the front of this card">		<media1 label="File" type="Media" optional="true" defaultValue="Select a File"/>		<tip1 label="Description" type="textInput" optional="true" defaultValue="" placeholder="Enter a description for accessibility"/>		<align1 label="Position" options="Top,Bottom" type="ComboBox" optional="true" data="Top,Bottom" defaultValue="Bottom" width="100"/>		<transcript1 label="Audio Transcript" type="textArea" height="100" defaultValue="" optional="true" tooltip="If media if MP3, a transcript button will appear on the audio player when text is added here"/>	</side1Media>		<side2Media type="group" label="Side 2 Media" optional="true" tooltip="Media (e.g. image) shown on the back of this card">		<media2 label="File" type="Media" optional="true" defaultValue="Select a File"/>		<tip2 label="Description" type="textInput" optional="true" defaultValue="" placeholder="Enter a description for accessibility"/>		<align2 label="Position" options="Top,Bottom" type="ComboBox" optional="true" data="Top,Bottom" defaultValue="Bottom" width="100"/>		<transcript2 label="Audio Transcript" type="textArea" height="100" defaultValue="" optional="true" tooltip="If media if MP3, a transcript button will appear on the audio player when text is added here"/>	</side2Media>	  </card></wizard>