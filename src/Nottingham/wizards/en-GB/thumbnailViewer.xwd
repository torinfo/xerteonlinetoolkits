<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>      <thumbnailViewer><![CDATA[<thumbnailViewer name="Enter Page Title" text="Enter text for the page" nextBtnTip="Next" priorBtnTip="Previous"/>]]></thumbnailViewer>    </newNodes>  </pageWizard>  <!-- THUMBNAIL VIEWER======================= -->  <thumbnailViewer menu="Media" menuItem="Thumbnail Viewer" hint="A series of thumbnail images for the user to browse." icon="icPictures" thumb="thumbs/thumb.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#thumbnailviewer" wiki="https://xerte.org.uk/wiki/doku.php?id=media_thumbnail_viewer_page" remove="true" deepLink="true">        <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="250"/>	    <nextBtnTip label="Next Button Tip" type="textInput" divider="true" language="true" />	    <priorBtnTip label="Prior Button Tip" type="textInput" language="true" />    <newNodes>      <thumbnailImage><![CDATA[<thumbnailImage name="Image" text="Content for the pane" align="Top" url="Select an Image" linktarget="none" link="Link URL" tip="Enter a Description for Accessibility"/>]]></thumbnailImage>    </newNodes>  </thumbnailViewer>    <thumbnailImage menuItem="Image" icon="icBullet" remove="true">    <name label="Label" type="TextInput" wysiwyg="true" />    <text label="Image Text" type="TextArea" height="250" />		<image type="group" label="Image">		<url label="File" type="media"/>		<tip label="Description" type="TextInput" mandatory="true" defaultValue="Enter a Description for Accessibility" />	</image>		<linkGroup label="Link" type="group">		<linktarget label="Link from image" conditionTrigger="true" options="None,Original image,Specified URL" type="ComboBox"  data="None,Original image,Specified URL" defaultValue="None" width="200"/>		<link label="URL" type="textInput" condition="linktarget=='Specified URL'"/>	</linkGroup>  </thumbnailImage>  </wizard>