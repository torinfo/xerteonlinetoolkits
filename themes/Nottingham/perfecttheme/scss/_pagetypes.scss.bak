//***********************************Text*************************************
//*****title page******
.titlePageTxt {
  //*without reducing the width here horizontal scroll bars show when the theme selected isn't 100% width*/
  width:96% !important;
  overflow: hidden;
}
//***********************************Media*************************************
//default highlight if not overriden
.highlight {
  /* background: $lightestcolor;  */
}
//*****audio slideshow********/
.x_audioSlideshow_page .slide {
  margin-left: 5px;
}
//*****multiple perspectives******/
#thumbHolder .thumbBox h3, .x_responsive #thumbHolder .thumbBox h3{
  color: $darkestcolor;
}
//*****Synched Video*******/
//add padding to right holder
#x_mainHolder.x_videoSynch_page .right{
  padding-top: 10px;
  padding-left: 20px;
}
//*****transcript reader******/
.x_transcriptReader_page .highlight {
  background:transparentize($whiteorlightest, .6);
}
//***********************************Navigators*************************************/
//*****Accordion page*******/
.ui-accordion .ui-accordion-content {
  /* border-radius: $smallroundedcorners;*/
  background: $lightestcolor;
  color:$blackordarkest;
}
.ui-accordion .ui-accordion-header {
  /* border-radius: $smallroundedcorners;*/
  background: $whiteorlightest;
  color:#e18538;
}
.ui-accordion .ui-accordion-header a {
  /* border-radius: $smallroundedcorners; */
  color: $whiteorlightest !important; //doesn't work without important
  background: $brightcolor;
}
.ui-accordion .ui-state-active a, .ui-state-active a:link, .ui-state-active a:visited {
  text-decoration: none;
  background: $accent1;
  /* border-radius: $smallroundedcorners; */
  color: $blackordarkest;
}

.ui-tabs-panel a:visited, .ui-accordion-content a:visited {
  color: $linkcolor;
}
.ui-tabs-panel a:link, .ui-accordion-content a:link {
  color: $linkcolor;
}

.ui-tabs-panel a:hover, .ui-accordion-content a:hover {
  color: $linkhover;
}

//*****button navigator*****/
.ui-button {
  /*border-radius: $smallroundedcorners;*/
  background: $contrastcolor;
  border: none;
  color: #ffffff;
}

.ui-button.ui-state-hover {
  background: $darkestcolor;
}

button.ui-button:disabled {
	background: $darkestcolor;
}
.ui-buttonset .ui-button {
  margin-left: 0;
  margin-right: .2em;
}
.ui-state-active{
  /*border-radius: $smallroundedcorners;*/
  background: $contrastcolor;
  border: none;
  color: $whiteorlightest;
}
//***Flash cards***/
.front.panel{
  background:$panelbackground;
  border:$panelbackground;
  color:$fontcolorforlightestcolor;
}
.back.panel{
  background:$panelbackground;
  border:$panelbackground;
  color:$fontcolorforlightestcolor;
}
//*****hyperlinks******/
.x_links_page #linkHolder a{
  color:$linkcolor;
}
.x_links_page #linkHolder a:visited{
  color:$linkvisited;
}
.x_links_page #linkHolder a:hover{
  color:$linkhover;
}
//*****navigator*****/
#pageContents.button button.selected {
  background-color: $lightestcolor;
}
#pageContents.slide #btnHolder button {
  background: $darkestcolor;
  border-color: $darkestcolor;
}
//****tabbed navigator****/
.ui-tabs .ui-tabs-nav li {
   /*border-radius: $smallroundedcorners;*/
   background: $brightcolor;
   border:none;
   color:$fontcolorforcontrastcolor;
 }
.ui-tabs .ui-tabs-nav .ui-state-active li{
  /*border-radius: $smallroundedcorners;*/
  background: $contrastcolor;
  border: none;
  color: $whiteorlightest;
}
.paneInfo{
  /*color:$fontcolorfobrightcolor;*/
}
.paneList a, .paneList a:link, .paneList a:visited{
  /*color: $linkcolor !important;*/
}
.ui-tabs-panel a:link, /*.ui-accordion-content a:link*/ {
  /*color: $linkcolor;*/
}
.ui-tabs .ui-tabs-nav {
  /* border-radius: $smallroundedcorners;*/
  background: none;
  border: none;
}
.ui-tabs .ui-tabs-nav li a {
  color: $whiteorlightest;
}
.ui-state-active a, .ui-state-active a:link, .ui-state-active a:visited {
  text-decoration: none;
  background: $contrastcolor !important;
  color:$whiteorlightest !important;
  /* border-radius: $smallroundedcorners; */
  border: none;
}



.x_accNav_page .ui-state-active a, .x_accNav_page .ui-state-active a:link,.x_accNav_page .ui-state-active a:visited {
   background: $brightcolor;
 }

.x_nav_page .ui-accordion .ui-state-active a, .x_nav_page .ui-accordion .ui-state-active a:link,.x_nav_page .ui-accordion .ui-state-active a:visited {
   background: $brightcolor;
 }
 
.x_nav_page .ui-button.selected {
    background-color: $contrastcolor !important;
}

a.topic.highlight {
  background: $contrastcolor;
  color:$fontcolorfobrightcolor;
}
a.topic.highlight:hover {
  background: $lightestcolor;
  color:$fontcolorforlightestcolor;
}
div.infoHolder.ui-tabs.ui-widget.ui-widget-content.ui-corner-all{
  background:$lightestcolor;
}
//***********************************Interactivity*************************************/
//most text areas unless changed for specifc pages
#pageContents textarea {
 /* border-radius: $mediumroundedcorners;*/
  padding: 10px;
}
//most input boxes unless changed for specifc pages
#pageContents input {
  border-radius: $noroundedcorners;
  padding-left: 10px;
  border-color: $darkestcolor 0.5;
}
//*****annotated diagram******/
#listholder a.listitem, a:visited {
  color:$contrastcolor ;
}
.x_annotatedDiagram_page .highlight{
  background:$panelbackground;
}
.x_annotatedDiagram_page .hotspot{
  background:none;
}
.x_annotatedDiagram_page #infoHolder{
  background:$lightestcolor;
  margin-left:10px;
}
//*******Drag and Drop Labelling*********/
.target.ui-droppable.border.highlight {
  //show transparent background when clicking target hotspots
  background-color: transparentize($lightestcolor, .6) !important;
}
.x_dragDropLabel_page #infoHolder{
  padding:20px;
  margin-right:20px;
  background: $whiteorlightest;
}
.x_dragDropLabel_page .label.panel{
  background:transparentize($darkestcolor, .2 )!important;
  border-color:$darkestcolor;
  color:$whiteorlightest;
}
//*****categories*****/
#categoryHolder .category {
  /*border-radius: $mediumroundedcorners;*/
}
#categoryHolder h3 {
  color:$blackordarkest;
}
.x_categories_page #dragDropHolder {
  background: $whiteorlightest;
  padding:10px;
  border-radius: $mediumroundedcorners;
}
.x_categories_page .label.panel{
  background:$darkestcolor;
  border:$darkestcolor;
  color:$whiteorlightest ;
}
#categoryHolder .highlight {
	background: $panelbackground;
 /* border-radius: $smallroundedcorners;*/
 /* background: $lightestcolor;*/
}
//*******decision tree********//
.x_decision_page .highlight{
  background: $lightestcolor;
}
//*****Dictation*****/
.x_dictation_page #answerTxt {
  width: 95%;
  margin-bottom: 10px;
  /* border-radius: $mediumroundedcorners;*/
  padding:10px;
}
//*****gap fill*****/
#targetHolder .target {
  /* border-radius: $mediumroundedcorners;*/
}
#targetHolder .highlight {
  /* background:$whiteorlightest;*/
}
#labelHolder .label.panel {
  //background: purple;
}
.x_gapFill_page #targetHolder .highlight {
  background:transparentize($whiteorlightest, .5);
}
.x_gapFill_page .label.panel {
  background: $darkestcolor;
  color:$whiteorlightest;
}
.x_gapFill_page .target.correct{
  color:green;
  font-weight:bold;
  background-color:transparentize($whiteorlightest, .8);
}
//*****hotspot image*****/
a.hotspot, a.hotspot.selected {
  background: none;
}
.hsinfo{
  padding:1%;
}

.x_hotspotImage_page #infoHolder  {
    background: $whiteorlightest;
}

//******Interactive lists*******/
.x_interactiveText_page .highlight{
  background: $lightestcolor;
}
//******Interactive lists*******/
.x_list_page .highlight{
  background: $contrastcolor;
  color:$fontcolorforcontrastcolor;
}
//*********matching texts*********/
.x_textMatch_page #dragDropHolder {
  background: $lightestcolor;
  padding:10px;
  border-radius: $mediumroundedcorners;
}
.x_textMatch_page .label.panel{
  background:$brightcolor;
  border-color:$brightcolor;
  color:$whiteorlightest;
}

//*****model answer*****/
.x_modelAnswer_page #answerTxt {
  /* border-radius: $mediumroundedcorners;*/
  width: 96%;
}
//*****opinion********/
.x_opinion_page #pageContents input {
  padding-left: 0;
}
.qPanel {
  overflow-x:hidden;
}
.qSeparator hr {
  background-color: lighten($lightestcolor, 15%);
  border-color: lighten($lightestcolor, 15%);
  border: solid 1px lighten($lightestcolor, 15%);
}
.sliderHolder .labelHolder a p {
  color: $blackordarkest;
}

//*****sortable grid*****/
.x_grid_page #gridBorders {
  border-radius: $smallroundedcorners;
}
.x_grid_page #grid .listHolder.constrainC li {
  border-radius: $smallroundedcorners;
}

.x_grid_page table.simple {
  border: transparent;
}
.x_grid_page table.header.simple tr:first-child{
  border-bottom: transparent;
  background:$darkestcolor;
}
//the draggable items
.x_grid_page #grid ul li{
  background:$brightcolor;
  border-color: transparent;
  color:$whiteorlightest ;
}
.x_grid_page #grid li.ui-state-default.shadow.ui-draggable.ui-droppable {
  color: $whiteorlightest !important;
}

.x_grid_page #grid ul li.header {
  color: $whiteorlightest ;
  background:$accent1;
}

//even rows of the table
.x_grid_page table tr.shaded {
  background: $contrastcolor;
  color:$blackordarkest !important;
}
//odd rows of the table
.x_grid_page table tr {
  background: $lightestcolor;
  color:white !important;
}
.x_grid_page #grid ul li.static {
  background: none;
  border-color: transparent;
}
/*****timeline matching pairs*****/
.x_timeline_page #dragDropHolder {
  background: $whiteorlightest;
}
//the target boxes
.x_timeline_page #targetHolder .target {
  /* border-radius: $mediumroundedcorners;*/
  background-color:$panelbackground;
}
.x_timeline_page .label.panel.ui-draggable {
  background: $darkestcolor;
  color: $fontcolorfobrightcolor;
  border-color:$darkestcolor;
}
//***********************************Connectors*************************************/
//*****menu connector*****/
#subMenuItems .subMenuItem {
  background: $darkestcolor;
  color:$fontcolorforcontrastcolor;
}

#subMenuItems .subMenuItem.ui-state-hover {
  background: $contrastcolor;
}
//***********************************Charts*************************************/
//***Charts Table***/
.x_table_page tr {
  background: $contrastcolor;
  color:$fontcolorforcontrastcolor;
}
.x_table_page table tr.shaded {
  background: $brightcolor;
  color:$fontcolorfobrightcolor;
}
.x_table_page table.header.shaded tr:first-child {
  background: $blackordarkest;
  color:$whiteorlightest;
}
.x_table_page table.header.simple tr:first-child {
  border:none;
}
.x_table_page table.simple {
  border: none;
}
//***********************************Games*************************************/
//*****games memory*****/
#pageContents .card {
  border-radius: $smallroundedcorners;
  border-color: $contrastcolor;
}
#cardTopHolder .card{
  border-color: $contrastcolor;
}
//*****games puzzle*****/
#puzzle {
  border-radius: $mediumroundedcorners;
}
//*****memory******/
#pageContents .card {
  color: $darkestcolor;
}
#cardTopHolder .card {
  background-image: url('card_back.svg') !important; //doesn't work without important
}
//***********************************Misc*************************************/
//*****youtube feed page*****/
.x_youtuberss_page #resultsHolder div {
  width:97%;
  min-height: 50px;
  padding-top: 5px;
  border-radius: $mediumroundedcorners;
  background-color: $lightestcolor;
  margin:.5em;
}
.x_youtuberss_page .shadedDiv {
   background-color: lighten($lightestcolor, 20%) !important; //doesn't work without important
   border-radius: $mediumroundedcorners;
 }
.x_youtuberss_page .shadedDiv p{
  color:$blackordarkest !important;
}
//*****qr code******/
div#shorturltodisplay a{
  color:$darkestcolor;
}
//*****RSS feed******/
.x_rss_page #resultsHolder div {
  /*border-radius: $mediumroundedcorners;*/
  background-color: $lightestcolor;
  margin-bottom: 2px;
}
.x_rss_page #resultsHolder div p {
  color: $blackordarkest;
}
.x_rss_page #resultsHolder .shadedDiv {
  background-color: lighten($lightestcolor, 20%) !important; //doesn't work without important
  /*border-radius: $mediumroundedcorners;*/
}
.x_rss_page #resultsHolder div a {
  color: darkblue;
}
//*****results page*****/
.x_results_page td, th {
  /* border-radius: $mediumroundedcorners;*/
  background:$lightestcolor;
}
.x_results_page #questionScores  th{
  background-color: $darkestcolor;
}
.x_results_page tr.alt td{
  background: $panelbackground;
  color:$mainfontcolour;
}
.x_results_page .details th {
  background: $darkestcolor;
  color: $fontcolorfordarkestcolor;
}
.x_results_page #general_summary th {
  background: #ffffff;//hide otherwise black line at to of summary table
}
//***********************************Media query/responsive changes*************************************/
//*****change tick/cross size on all pages where used******/
.tick, img.result{
  @media (min-width: 768px) {
    width: 2.5vmin;
  }
}

